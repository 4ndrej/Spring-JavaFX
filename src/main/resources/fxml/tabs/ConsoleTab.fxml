<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.Bloom?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<Tab fx:id="consoleTab" text="Console" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jcchv.java.controllers.ConsoleTabController">
    <content>
        <Pane prefHeight="398.0" prefWidth="714.0">
            <children>
                <Label fx:id="lblSpring" alignment="CENTER" contentDisplay="CENTER" layoutX="233.0" layoutY="107.0" text="Spring Boot - Java FX">
                    <font>
                        <Font size="24.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="TEXT" />
                    </cursor>
                    <effect>
                        <Bloom />
                    </effect>
                </Label>
                <Button fx:id="btnSpring" alignment="CENTER" contentDisplay="CENTER" layoutX="304.0" layoutY="229.0" mnemonicParsing="false" onAction="#sayHello" text="Say hello!" textAlignment="CENTER">
